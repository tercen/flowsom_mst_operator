{
  "name": "FlowSOM MST",
  "description": "Generate plots of FlowSOM Minimum Spanning Trees.",
  "tags": ["data visualisation"],
  "authors": ["tercen"],
  "container": "tercen/flowsom_mst_operator:0.0.3",
  "urls": ["https://github.com/tercen/flowsom_mst_operator"],
  "properties": [
  {
    "kind": "DoubleProperty",
    "name": "plot.width",
    "defaultValue": 750,
    "description": "Plot width (pixels)"
  },
  {
    "kind": "DoubleProperty",
    "name": "plot.height",
    "defaultValue": 750,
    "description": "Plot height (pixels)"
  },
  {
    "kind": "BooleanProperty",
    "name": "plot.markers",
    "defaultValue": false,
    "description": "shall individual marker plots be displayed?"
  },
  {
    "kind": "DoubleProperty",
    "name": "maxNodeSize",
    "defaultValue": 1.2,
    "description": "Maximum node size."
  }
  ]
}
